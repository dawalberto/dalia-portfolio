---
import Icon from "@/components/Icon.astro"
import { ATID_PROJECT, AtidNavSections } from "@/constants"
import Project from "@/layouts/Project.astro"

const SECTION_1 = ATID_PROJECT.navItems.get(AtidNavSections.SECTION_1)
const SECTION_2 = ATID_PROJECT.navItems.get(AtidNavSections.SECTION_2)
const SECTION_3 = ATID_PROJECT.navItems.get(AtidNavSections.SECTION_3)
const SECTION_4 = ATID_PROJECT.navItems.get(AtidNavSections.SECTION_4)
const SECTION_5 = ATID_PROJECT.navItems.get(AtidNavSections.SECTION_5)
const SECTION_6 = ATID_PROJECT.navItems.get(AtidNavSections.SECTION_6)
const SECTION_7 = ATID_PROJECT.navItems.get(AtidNavSections.SECTION_7)
---

<!-- Proyecto de EmpatÃ­a Animal - Kingsugi -->
<Project
  title={ATID_PROJECT.title}
  rol={ATID_PROJECT.rol}
  duration={ATID_PROJECT.duration}
  typeOfProject={ATID_PROJECT.typeOfProject}
  tools={ATID_PROJECT.tools}
  navItems={ATID_PROJECT.navItems}
>
  <div>
    <section id={AtidNavSections.SECTION_1}>
      <h2>
        {SECTION_1?.icon && <Icon icon={SECTION_1.icon} />}
        <span>{SECTION_1?.title}</span>
      </h2>
    </section>
    <section id={AtidNavSections.SECTION_2}>
      <h2>
        {SECTION_2?.icon && <Icon icon={SECTION_2.icon} />}
        <span>{SECTION_2?.title}</span>
      </h2>
    </section>
    <section id={AtidNavSections.SECTION_3}>
      <h2>
        {SECTION_3?.icon && <Icon icon={SECTION_3.icon} />}
        <span>{SECTION_3?.title}</span>
      </h2>
    </section>
    <section id={AtidNavSections.SECTION_4}>
      <h2>
        {SECTION_4?.icon && <Icon icon={SECTION_4.icon} />}
        <span>{SECTION_4?.title}</span>
      </h2>
    </section>
    <section id={AtidNavSections.SECTION_5}>
      <h2>
        {SECTION_5?.icon && <Icon icon={SECTION_5.icon} />}
        <span>{SECTION_5?.title}</span>
      </h2>
    </section>
    <section id={AtidNavSections.SECTION_6}>
      <h2>
        {SECTION_6?.icon && <Icon icon={SECTION_6.icon} />}
        <span>{SECTION_6?.title}</span>
      </h2>
    </section>
    <section id={AtidNavSections.SECTION_7}>
      <h2>
        {SECTION_7?.icon && <Icon icon={SECTION_7.icon} />}
        <span>{SECTION_7?.title}</span>
      </h2>
    </section>
  </div>
</Project>
